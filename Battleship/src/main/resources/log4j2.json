{
  "configuration": {
    "appenders": {
      "Console": {
        "name": "STDOUT",
        "PatternLayout": {
          "pattern": "%5p %d - %n%m%n"
        }
      }
    },
    "loggers": {
      "logger": {
        "name": "EventLogger",
        "level": "info",
        "additivity": "false",
        "appender-ref": {
          "ref": "Routing"
        }
      },
      "root": {
        "level": "debug",
        "appender-ref": {
          "ref": "STDOUT"
        }
      }
    }
  }
}
